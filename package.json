    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build2": "ng build",
        "watch": "ng build --watch",
        "build-prod": "ng build --prod --build-optimizer --stats-json",
        "analyze-show": "webpack-bundle-analyzer www/stats.json",
        "test": "ng test",
        "lint": "ng lint",
        "precommit2": "npm run lint",
        "platform": "ionic cordova platform add android --save",
        "rm": "rimraf ./platforms/android ./plugins ./node_modules",
        "unplatform": "ionic cordova platform rm android --save && rimraf ./plugins",
        "live": "ionic cordova run android -lc",
        "dev": "ionic cordova run android",
        "build": "ionic cordova build android",
        "release": "ionic cordova build android --release"
    },
